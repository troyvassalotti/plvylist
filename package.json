  "bugs": {
    "url": "https://github.com/troyvassalotti/plvylist/issues"
  },
  "homepage": "https://github.com/troyvassalotti/plvylist#readme",
  "dependencies": {
    "@lit/task": "^1.0.1",
    "lit": "^3.1.4"
  },
  "devDependencies": {
    "cem-plugin-async-function": "^0.0.5",
    "cem-plugin-jsdoc-example": "^0.0.9",
    "common-tags": "^1.8.2",
    "custom-element-jet-brains-integration": "^1.5.0",
    "custom-element-vs-code-integration": "^1.3.0",
    "markdown-it": "^14.1.0",
    "prettier": "^2.5.1",
    "vite": "^4.0.4",
    "vite-plugin-cem": "^0.8.0",
    "vite-plugin-nunjucks": "^0.2.0"
