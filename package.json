    "vite::docs": "npx vite build --mode docs",
    "preview::docs": "npx vite --mode docs",
    "prebuild": "npm run prettier::format",
    "build": "npm run vite::build",
    "prettier::lint": "npx prettier --check .",
    "prettier::format": "npx prettier --write ."
  },
  "author": "Troy Vassalotti (https://www.troyv.dev/)",
