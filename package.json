{
  "type": "module",
  "name": "plvylist",
  "version": "4.0.0",
  "description": "A web component for playing an album or multiple tracks in your site or app.",
  "main": "dist/plvylist.js",
  "exports": {
    ".": "./dist/plvylist.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/troyvassalotti/plvylist.git"
  },
  "keywords": [
    "web component",
    "music",
    "audio"
  ],
  "scripts": {
    "start": "npx vite",
